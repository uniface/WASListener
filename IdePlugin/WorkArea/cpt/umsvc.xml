public operation retrieveData
throws
; This operation retrieves data from database and returns it as a struct.
; Data could also be returned in several other (serialized) formats, like:
; - XML
; - JSON
; - Uniface list
; - ...
params
  string pProfiles: in    ; Associated list containing profile in format:
                          ; "FIELDNAME1=PROFILE1{&uSEP;FIELDNAME2=PROFILE2{&uSEP;...}}"
  byref struct pData: out ; Data returned in a struct reference type
endparams

  ; Load the profile data
  clear/e "MYENTITY"
  getlistitems/occ/init pProfiles, "MYENTITY"

  ; Optionally validate your profile data here...

  ; Retrieve data from database
  try
    retrieve/e "MYENTITY"
  catch &lt;UIOSERR_OCC_NOT_FOUND&gt;, &lt;UIOSERR_OPEN_FAILURE&gt;
    ; Retrieve: No data found in the database
    return -1
  endtry

  ; Load data into a struct. Use /reconnecttags switch to add occurrence metadata information,
  ; which will be used by Uniface to be able to reconnect disconnected occurrences.
  componentToStruct/reconnecttags pData, "MYENTITY"
  ; Optionally serialize to different format, e.g.:
  ; vData = ustruct::toList(pData)
  ; vData = ustruct::toXML(pData)
  ; vData = ustruct::toJSON(pData)

  return 0

end


public operation updateData
throws
; This operation updates input data in the database and returns data errors encountered during data reconnect, validation or store.
; This operation takes a struct as input, but data could also be provided in several other (serialized) formats, like:
; - XML
; - JSON
; - Uniface list
; - ...
; This operation returns an exception when update fails
params
  byref struct pData: in
endparams
variables
  struct  vData, vDataErrors
  string  vInput, vEntityName, vProcerrorcontext
endvariables

  ; Optionally, deserialize data first, e.g.:
  ; Deserialize/parse exceptions might be thrown
  ; vData = ustruct::fromList(vInput)
  ; vData = ustruct::fromXML(vInput)
  ; vData = ustruct::fromJSON(vInput)

  ; Load struct data into the component
  try
    structtocomponent pData
  catch &lt;USTRUCTERR_INPUT&gt;  ; Input struct data is not valid
    ; $procreturncontext contains more error information, so add that information to exception
    vProcerrorcontext = $procerrorcontext
    putitem/id vProcerrorcontext, "PROCRETURNCONTEXT", $procreturncontext
    throw &lt;USTRUCTERR_INPUT&gt;, $item("DESCRIPTION", vProcerrorcontext), vProcerrorcontext
  endtry

  ; Data that is generated by Uniface includes reconnect information, reconnect data with database and other data already in instance.
 $instancehandle()-&gt;reconnect("MYENTITY")

  ; Validate and Store
  try
    ; Validate data before store to separate validation errors from store errors
    validate/e "MYENTITY"
    try
      store/e "MYENTITY"
      commit
    catch
      vProcErrorContext = $procerrorcontext
      rollback
      throw $item("ERROR", vProcErrorContext), "Rethrow: store failed", vProcErrorContext
    endtry
  catch &lt;UGENERR_4GL_SAYS_ERROR&gt;, &lt;UVALERR_SYNTAX&gt;, &lt;UVALERR_CONSTRAINT&gt;, &lt;UVALERR_KEY_PROFILE&gt;, &lt;UVALERR_KEY_EMPTY&gt;
    ; Vaidation errors detected, errors are reported by error trigger and added to entity data
    ; Get errors and add as details to exception:
    $collhandle("MYENTITY")-&gt;reportDataErrors(vDataErrors)
    throw &lt;UGENERR_4GL_SAYS_ERROR&gt;, "Validation failed", ustruct::toList(vDataErrors)
  endtry
  
end


partner operation reconnect
throws
params
  string pEntityName: in
endparams
variables
  struct  vDataErrors
endvariables

  reconnect pEntityName
  if ($status &gt; 0)
    ; Reconnect errors detected, errors are reported by error trigger and added to entity data 
    try
      ; Get errors and add as details to exception:
      $collhandle(pEntityName)-&gt;reportDataErrors(vDataErrors)
    catch &lt;UPROCERR_OPERATION&gt;
      ; Operation to generate error report is not available -&gt; add simple error report
      vDataErrors-&gt;"(%%pEntityName)" = "Reconnect failed"
    endtry
    throw -310, "Reconnect failed", ustruct::toList(vDataErrors)
  endif

end
