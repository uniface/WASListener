<DAT name="UCOLL_SCRIPT" xml:space='preserve'>
partner operation exportAll
	setocc "VC_UPDATED.VC_VIEWS", 1
	forentity  "VC_UPDATED.VC_VIEWS"
		$occhandle("VC_UPDATED.VC_VIEWS")-&gt;export()
	endfor
end

partner operation revertAll
	setocc "VC_UPDATED.VC_VIEWS", 1
	forentity  "VC_UPDATED.VC_VIEWS"
		$occhandle("VC_UPDATED.VC_VIEWS")-&gt;revert()
	endfor
end

</DAT>
<DAT name="UOCC_SCRIPT" xml:space='preserve'>trigger read
	variables
	  string vUSSPEC, vUSIMPL
	endvariables

	read
	if ($status &gt;= 0 &amp; !INCLUNISIGS.ENTITY)
		if ($lowercase(TYPE.VC_UPDATED) = "sig")
			selectdb UDEFPATH from "USSPEC" u_where (USPECNAM.USSPEC = NAME.VC_UPDATED) to (vUSSPEC)
			selectcase $lowercase(vUSSPEC)
				case "frm", "svc", "rpt", "esv", "ssv", "usp", "dsp"
					selectdb UPATH from "USIMPL" u_where (USPECNAM.USIMPL = NAME.VC_UPDATED) to (vUSIMPL)
					if ($itemcount(vUSIMPL) = 1)
						discard
					endif
			endselectcase
		endif
	endif
end

partner operation export
	variables
		string vName, vNamespace
		handle vInterface
	endVariables

	if ( !$dbocc ) return -1

	vName = NAME.VC_UPDATED
	vNameSpace = NAMESPACE.VC_UPDATED

	newinstance $concat("VC_", TYPE.VC_UPDATED), vInterface

	if (ACTION.VC_UPDATED = "del")
		vInterface-&gt;deleteWAS(vName, vNamespace)
	else
		vInterface-&gt;export(vName, vNamespace)
	endif

	$fieldsyntax(EXPORT) = "DIM"
	$fieldsyntax(REVERT) = "DIM"
end

partner operation revert
	variables
		string vName, vNamespace
		handle vInterface
	endVariables

	if ( !$dbocc ) return -1

	vName = NAME.VC_UPDATED
	vNameSpace = NAMESPACE.VC_UPDATED

	newinstance $concat("VC_", TYPE.VC_UPDATED), vInterface

	vInterface-&gt;revert(vName, vNamespace)
	$fieldsyntax(EXPORT) = "DIM"
	$fieldsyntax(REVERT) = "DIM"
	$needs_refresh$ = 2
end
</DAT>
