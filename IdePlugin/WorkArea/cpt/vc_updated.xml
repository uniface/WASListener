<DAT name="USCRIPT" xml:space='preserve'>operation exec
	params
		string  pRedirectionUrl :  out
		numeric $needs_refresh$ : out
	endparams

	; On the whole we don't need to refresh as we are just exporting
	; Revert does need to refresh - it will be set in the occurence operation revert of VC_UPDATED
	$needs_refresh$ = 0
	clear
	retrieve
	if ( $status &lt; -2 )
		activate "VC_MAIN".createTables()
		retrieve
	endif

	edit

	return 0
end

public operation getVersion 
  params
    string pApiVersion: out
  endparams
		
  pApiVersion=1
		
  return 0
end; operation getVersion

public operation getGlobalMenuOptions
	params
		struct pMenu: out 
	endparams

	pMenu-&gt;$name = "menu"
	call addOption(pMenu, "WorkArea Export/Revert", $componentname, "exec", 1)
	call addOption(pMenu, "Import WorkArea", $componentname, "loadFromWA", 1)
	call addOption(pMenu, "Mark all as ready for export", $componentname, "resetWASstamps", 1)
end ; operation getGlobalMenuOptions

public operation getEditorMenuOptions
	params
		string pObjectUrl: in
		struct pMenu: out
	endparams

	return 0
end ; operation getEditorMenuOptions

operation loadFromWA
	params
		string  pRedirectionUrl :  out
		numeric pNeedsRefresh   :  out
	endparams
	variables
		handle vVc
	endVariables
	
	activate "IDE_API_1".outputMessage("Importing from Workarea")
	newinstance "VC_MAIN", vVc
	vVc-&gt;loadFromWA()

	pNeedsRefresh = 2
end

operation resetWASstamps
	params
		string  pRedirectionUrl :  out
		numeric pNeedsRefresh   :  out
	endparams
	variables
		handle vVc
	endVariables

	askmess "This action will mark all repository obects as needing exporting."
	if ( $status = 0 )
		return
	endif

	activate "IDE_API_1".outputMessage("Resetting all repository objects")
	newinstance "VC_MAIN", vVc
	vVc-&gt;resetWASstamps()

	pNeedsRefresh = 2
end

trigger clear
; This trigger is fired by a macro ^CLEAR (GOLD-G) and typically cleares the data from the component without storing

  ; Your implementation here, e.g.:
  clear

  return 0
end

trigger retrieve
; This trigger is fired on macro ^RETRIEVE (GOLD-R) and typically performs a data retrieve

  ; Your implementation here, e.g.:
  retrieve
  selectcase $procerror
  case 0
    ; ok
  case -2, -4
    message/info "Retrieve: No data found in the database."
  elsecase
    message/error "Retrieve: Error occurred - (%%($procerror))."
  endselectcase

  return 0
end

entry addOption
  params
    struct pMenu:  in
    string pLabel: in
    string pComponentName: in
    string pCallbackOperation: in
    numeric pEnabled: in
  endparams

  variables
    struct vOption
  endvariables
				
  pMenu-&gt;option{-1} = $newstruct  ;- add a new option
  vOption = pMenu-&gt;option{-1}
  vOption-&gt;label = pLabel
  vOption-&gt;component = pComponentName
  vOption-&gt;operation = pCallbackOperation
  vOption-&gt;enabled = pEnabled
				
  return 0
end ;- entry addOption</DAT>
