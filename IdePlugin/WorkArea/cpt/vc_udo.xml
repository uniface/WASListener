<DAT name="USCRIPT" xml:space='preserve'>operation resetWASstamps
	clear

	retrieve/e "&lt;VC_STAMPS&gt;"
	if ( $status &lt; 0 )
		return 0
	endif

	; Get all data
	setocc "&lt;VC_STAMPS&gt;", -1
	setocc "&lt;VC_STAMPS&gt;", 1

	erase/e "&lt;VC_STAMPS&gt;"
	if ($status &lt;0)
		message "Erase error; see message frame"
		rollback
	else
		if ($status = 1)
			message "Erase is not allowed"
		else
			message "Erase was successful"
			commit
			if($status &lt; 0)
				rollback
			endif
		endif
	endif
end

entry lpCalculateWASObjectLocation
	returns string
	variables
		string vFolder
	endVariables

	activate "VC_MAIN".getWASFolder(vFolder)
	if ( $status &lt; 0 )
		vFolder = ".\"
	endif

	return $concat( vFolder, "\&lt;OBJECT_FOLDER&gt;")
end

entry lpExportProfile
	params
		string pName      : IN
		string pNamespace : IN
		string pProfile1  : OUT
		string pProfile2  : OUT
	endParams

#if ("&lt;OBJECT_TYPE&gt;" == "library" | "&lt;OBJECT_TYPE&gt;" == "field") 
	pProfile1 = "&uALL;"
	pProfile2 = $concat("library=", pName)
#elseif ("&lt;OBJECT_TYPE&gt;" == "signature")
	pProfile1 = 
	pProfile2 = "inclCompDef=False
#elseif ("&lt;OBJECT_TYPE&gt;" == "entity" | "&lt;OBJECT_TYPE&gt;" == "model")
	pProfile1 = pName
	pProfile2 = $concat("model=", pNamespace)
#else
	pProfile1 = pName
	pProfile2 = ""
#endif

	return
end

entry lpFileName
	returns string
	params
		string pName      : IN
		string pNamespace : IN
	endParams
	variables
		string vFilename, vFolder
	endVariables

	vFolder = lpCalculateWASObjectLocation()

	if (!$fileexists(vFolder)) ldircreate vFolder

	vFileName = $concat(vFolder, "\", $lowercase(pName))
	if ( pNamespace )
		vFileName = $concat(vFileName, ".", $lowercase(pNamespace))
	endif
	vFileName = $concat(vFileName, ".xml")
	return vFileName
end

entry lpSetProfile
	params
		string pName      : IN
		string pNamespace : IN
	endParams

	if ( pName == "" ) return -1
	&lt;NAME.&lt;MAIN_UDO&gt;&gt;/init = $uppercase(pName)

	if ( pNamespace )
		&lt;NAMESPACE.&lt;MAIN_UDO&gt;&gt;/init = $uppercase(pNamespace)
	endif

end

entry lpGetObject
	params
		string pName      : IN
		string pNamespace : IN
	endParams

	clear

	call lpSetProfile(pName, pNamespace)

	retrieve/e "&lt;MAIN_UDO&gt;"
	if ( $status &lt; 0 ) 
		clear
		return (&lt;UDO_NOT_FOUND&gt;)
	endif
	setocc "&lt;MAIN_UDO&gt;", 2
	if ( $status &gt; 0 ) 
		clear
		putmess "Profile %%(pName).%%(pNamespace) found multiple rows &lt; 2 were expected (%%($componentname))"
		return (&lt;ERR_MULTIPLE_UDOS_FOUND&gt;)
	endif
	return(&lt;SUCCESS&gt;)

end

public operation deleteWAS
	params
		string pName      : IN
		string pNamespace : IN
	endParams
	variables
		string vFilename
	endvariables

	lfiledelete lpFileName(pName, pNamespace)

	call lpSetProfile(pName, pNamespace)
	retrieve/e "&lt;VC_STAMPS&gt;"
	remocc "&lt;VC_STAMPS&gt;"
	store/e "&lt;VC_STAMPS&gt;"
	call lpStoreStatus
	if ( $status &lt; &lt;SUCCESS&gt; )
		clear
		return &lt;ERR_DELETE_FAILED&gt;
	endif
	clear
	return(&lt;SUCCESS&gt;)
end

public operation delete
	params
		string  pName      : IN
		string  pNamespace : IN
		boolean pForce     : IN
	endParams

	call lpGetObject(pName, pNamespace)
	if ( $status != &lt;SUCCESS&gt; ) done

	if ($logical("VC_LOGGING"))
		putmess "Component in Repository: %%(&lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt;)"
		putmess "Component date exported: %%(&lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt;)"
		putmess "Component date star: %%(&lt;EXPORTSTARTSTAMP.&lt;VC_STAMPS&gt;&gt;)"
	endif

	if (&lt;EXPORTSTARTSTAMP.&lt;VC_STAMPS&gt;&gt; &gt; $datim - 3s ) ; Still exporting
		if ($logical("VC_LOGGING"))
			putmess "Exporting - skiping file %%(&lt;EXPORTSTARTSTAMP.&lt;VC_STAMPS&gt;&gt;)"
		endif
		clear
		return (&lt;UDO_EXPORTED&gt;)
	endif

	setocc "*", -1
	setocc "*", 1

	if (pForce || (&lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt; == &lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt;))
		remocc "&lt;MAIN_UDO&gt;"
		store
		call lpStoreStatus
		if ( $status &lt; &lt;SUCCESS&gt; )
			clear
			return &lt;ERR_DELETE_FAILED&gt;
		endif
	else
		if ($logical("VC_LOGGING"))
			putmess "Component in Repository: %%(&lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt;)"
			putmess "Component date exported: %%(&lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt;)"
		endif
		clear
		return (&lt;DIRTY&gt;)
	endif

	clear
	return (&lt;SUCCESS&gt;)
end

public operation revert
	params
		string pName      : IN
		string pNamespace : IN
	endParams
	variables
		string vFilename
	endVariables

	vFilename = lpFileName(pName, pNamespace)

	$instancehandle-&gt;delete(pName, pNamespace, 1)
	$instancehandle-&gt;import(vFilename, pName, pNamespace)
	
end

public operation import
	params
		string pFilename  : IN
		string pName      : IN
		string pNamespace : IN
	endParams
	variables
		numeric vResult
	endVariables

	;if there is no file then there is nothing to import
	if ( !$lfileexists(pFilename) ) return(&lt;NOTHING_TO_IMPORT&gt;)

	; Must not exist
	call lpGetObject(pName, pNamespace)
	if ( $status = &lt;SUCCESS&gt; ) return &lt;ERR_OBJECT_ALREADY_EXISTS&gt;

	vResult = $ude("import", "misc", pFilename, "", "map=#")
	if ( $procerror &lt; 0 ) 
		putmess "******************************************************************************************"
		putmess "******************************************************************************************"
		putmess "Import failed:"
		putmess $procerrorcontext
		putmess "******************************************************************************************"
		putmess "******************************************************************************************"

		return (&lt;ERR_IMPORT_FAILED&gt;)
	endif

	call lpGetObject(pName, pNamespace)
	if ( $status &lt; &lt;SUCCESS&gt; ) 
		putmess "Object %%(pName).%%(pNamespace) does not exist after import"
		clear
		return (&lt;ERR_IMPORT_FAILED_NO_OBJECT&gt;)
	endif

	&lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt; = &lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt;
	store/e "&lt;VC_STAMPS&gt;"
	call lpStoreStatus
	if ( $status &lt; 0 )
		clear
		putmess "Unable to set VC UTIMESTAMP"
		return (&lt;ERR_IMPORT_STAMP_CREATE_FAILED&gt;)
	endif

	return vResult
end

public operation export
	params
		string pName      : IN
		string pNamespace : IN
	endParams
	variables
		string vProfile1, vProfile2, vFileName, vObjectType
		numeric vResult
	endVariables

	call lpGetObject(pName, pNamespace)
	if ( $status != &lt;SUCCESS&gt; ) done

	if ( &lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt; = "" )
		; the UTIMSTAMP needs to be set so if it isn't the make it now
		&lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt; = $datim
		store/e "&lt;MAIN_UDO&gt;"
		if ( $status &lt; 0 )
			clear
			putmess "Unable to set VC UTIMESTAMP"
			return &lt;ERR_EXPORT_FAILED&gt;
		endif
	endif

	&lt;EXPORTSTARTSTAMP.&lt;VC_STAMPS&gt;&gt; = $datim
	store/e "&lt;VC_STAMPS&gt;"
	call lpStoreStatus
	if ( $status &lt; 0 )
		clear
		putmess "Unable to set VC UTIMESTAMP"
		return &lt;ERR_EXPORT_FAILED&gt;
	endif

	call lpExportProfile(pName, pNamespace, vProfile1, vProfile2)
	vFilename = lpFileName(pName, pNamespace)

	#if ("&lt;OBJECT_TYPE_SUB&gt;" = "" )
		vResult = $ude ("export", "&lt;OBJECT_TYPE&gt;", vProfile1, vFilename, vProfile2 )
	#else
		vResult = $ude ("export", "&lt;OBJECT_TYPE&gt;&uSEP;&lt;OBJECT_TYPE_SUB&gt;", vProfile1, vFilename, vProfile2 )
	#endif

	&lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt; = &lt;UTIMESTAMP.&lt;MAIN_UDO&gt;&gt;
	store/e "&lt;VC_STAMPS&gt;"
	call lpStoreStatus
	if ( $status &lt; 0 )
		clear
		putmess "Unable to set VC UTIMESTAMP"
		return &lt;ERR_EXPORT_FAILED&gt;
	endif
end

entry lpSetDefUdo
	&lt;NAME.&lt;MAIN_UDO&gt;&gt; = "A"
	&lt;NAMESPACE.&lt;MAIN_UDO&gt;&gt; = "A"
end

public operation createTables
	params
		string pKeyFieldsUDO : OUT
		string pUdoEnt       : OUT
		string pKeyFieldsVC  : OUT
		string pVcEnt        : OUT
	endParams

	call lpSetDefUdo
	&lt;VC_UTIMESTAMP.&lt;VC_STAMPS&gt;&gt; = $datim

; Does not matter if this passes of fails as we are only interested in Uniface creating the table
	store 
	remocc "&lt;MAIN_UDO&gt;"
	store
	commit

	call lpKeyFields(pKeyFieldsUDO, pUdoEnt, pKeyFieldsVC, pVcEnt)
end

entry lpKeyFields
	params
		string pKeyFieldsUDO : OUT
		string pUdoEnt       : OUT
		string pKeyFieldsVC  : OUT
		string pVcEnt        : OUT
	endParams
	pKeyFieldsUDO = $keyfields("&lt;MAIN_UDO&gt;", 1) 
	pUdoEnt = "&lt;UNQ_MAIN_UDO&gt;"
	pKeyFieldsVC = $keyfields("&lt;MAIN_UDO&gt;", 1) 
	pVcEnt = "&lt;UNQ_VC_STAMPS&gt;"
end

entry lpStoreStatus
	variables
		numeric vProcError
		numeric vStatus
	endvariables

	vStatus = -1
	selectcase $status
	case 1
		; no modifications
		putmess "No modifications."
	case 0
		; store successful
		commit
		selectcase $procerror
			case 0
				; store and commit successful
				;message/info "Store successful."
				vStatus = &lt;SUCCESS&gt;
			elsecase
				; commit error
				putmess "Commit failed ($procerror=%%($procerror))."
		endselectcase
	elsecase
		; store failed
		vProcError = $procerror
		; rollback
		rollback
		selectcase $procerror
			case 0
			; rollback successful
			putmess "Store failed ($procerror=%%(vProcError)) - transaction rolled back."
		elsecase
			; rollback error
			putmess "Store failed ($procerror=%%(vProcError)) - Rollback failed ($procerror=%%($procerror))."
		endselectcase
	endselectcase

	return vStatus

end

