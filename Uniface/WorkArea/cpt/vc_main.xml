operation getWASFolder
	params
		string pFolder : OUT
	endPArams
 
	if ( $logical("WAS_ROOT_FOLDER") != "" )
		pFolder = $logical("WAS_ROOT_FOLDER")
	else
		pFolder = "&lt;OBJECT_LOCATION_ROOT&gt;"
	endif

	if ( $lfileexists(pFolder) = 0 )
		ldircreate pfolder
		if ( $status != 0 ) ; Failed to create the folder
			pfolder = ""
			return(-1)
		endif
	elseif ( $lfileexists(pFolder) != 2 ) ; not a folder
		pFolder = ""
		return (-2)
	endif

	return 0
end

