operation loadFromWA
	variables
		string vUdo, vFile, vFolder
	endVariables

	$instanceHandle-&gt;getWASFolder(vFolder)
	if ( $status &lt; 0 )
		message/error $concat(vFolder, "~WorkArea does not exist")
		return(-1)
	endif

	forlist vUDO in $ldirlist(vFolder, "DIR")
		forlist vFile in $ldirlist("%%(vFolder)\%%(vUDO)", "FILE")

			$instanceHandle-&gt;vc("%%(vFolder)\%%(vUDO)\%%(vFile)", 1, 1)

		endFor
	endfor
end

operation vc
	params
		string  pFileName : IN
		boolean pDelete   : IN
		boolean pImport   : IN
	endParams
	variables
		string  vWasFolder, vUdoType, vName, vNamespace
		boolean vDelete, vImport
		numeric vResult
		handle  vWorker
	endvariables

	if ($logical("VC_LOGGING"))
		putmess "---------------------------------------------------------------------------------------"
		putmess "Version control called:"
		putmess "	pFilename: %%(pFileName)"
		putmess "	pDelete: %%(pDelete)"
		putmess "	pImport: %%(pImport)"
	else
		$ioprint = 0
	endif

	call lpProfileFromFilename(pFileName, vWasFolder, vUdoType, vName, vNamespace)
	if ( $status != &lt;SUCCESS&gt; ) done

	selectcase vUdoType
#for ITEM = (&lt;UDO_TYPES&gt;)
	case "&lt;ITEM&gt;"
		newinstance "VC_&lt;ITEM&gt;", vWorker
#endfor
	elsecase
		; unknown
		return 0
	endselectcase

	;- Both delete and modify require the UDO to be removed
	if (pDelete)
		vWorker-&gt;delete(vName, vNamespace, 0)
		if ( $status = &lt;DIRTY&gt; )
			putmess "%%(vUdoType) %%(vName).%%(vNamespace): Dirty in repository"
			return &lt;DIRTY&gt;
		elseif ( $status &gt; &lt;SUCCESS&gt; )
			if ( $status = &lt;UDO_EXPORTED&gt;)
				if ($logical("VC_LOGGING"))
					putmess "%%(vUdoType) %%(vName).%%(vNamespace): Too recently exported"
				endif
				return &lt;UDO_EXPORTED&gt;
			else
				putmess "%%(vUdoType) %%(vName).%%(vNamespace): Not found in repository"
			endif
		elseif ( $status &lt; &lt;SUCCESS&gt; )
			putmess "%%(vUdoType) %%(vName).%%(vNamespace): Failed to delete"
			return &lt;ERR_DELETE_FAILED&gt;
		else
			putmess "Delete %%(vUdoType) %%(vName).%%(vNamespace)"
		endif
	endif
	

	; Both Adding and modifying needs to import the export
	if (pImport)
		putmess "Import %%(vUdoType) %%(vName).%%(vNamespace)"
		vWorker-&gt;import(pFilename, vName, vNamespace)
		if ( $status &lt; &lt;SUCCESS&gt; )
			putmess "%%(vUdoType) %%(vName).%%(vNamespace): Failed to import"
			return &lt;ERR_IMPORT_FAILED&gt;
		endif
	endif

	return &lt;SUCCESS&gt;
end

entry lpProfileFromFilename
	params
		string pFilename  : IN
		string pWasFolder : OUT
		string pUdoType   : OUT
		string pName      : OUT
		string pNamespace : OUT
	endParams
	variables
		numeric vSeparator, vNamespaceStarts
		string vFilename
	endvariables

	; Get the export file name
	vSeparator = $rscan(pFilename, "\") 
	vFilename  = pFilename[vSeparator + 1]
	pFilename  = pFilename[1:vSeparator - 1]
	
	; Get the WAS folder and the UDO type
	vSeparator = $rscan(pFilename, "\") 
	pUdoType   = pFilename[vSeparator + 1]
	pWasFolder  = pFilename[1:vSeparator - 1]

	; Decode the filename into the UDO name and Namespace
	vFilename  = vFilename[1 : $length(vFilename) - 4]
	vNamespaceStarts = $scan(vFilename, ".")
	if ( vNamespaceStarts &gt; 0 )
		pName = vFilename[1:vNamespaceStarts - 1]
		pNamespace = vFilename[vNamespaceStarts + 1]
	else
		pName = vFilename
	endif

	if (( !pWasFolder )|( !pUdoType )|( !pName )) return &lt;ERR_INVALID_FILENAME&gt;

	return &lt;SUCCESS&gt;
end

