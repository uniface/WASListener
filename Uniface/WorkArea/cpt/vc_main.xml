#startDefine ;CreateTables
public operation createTables
	variables
		handle vWorker
		string vViewSQL, vViewName
		string vKeyFieldsUDO, vUdoEnt
		string vKeyFieldsVC, vVcEnt
		string vSpecs
	endVariables

	activate "VC_SPECS".exec(vSpecs)
	vViewName = lpSQLID("VC_UPDATED", vSpecs)

#for ITEM = (&lt;UDO_TYPES&gt;), COUNTER
	newinstance "VC_&lt;ITEM&gt;", vWorker
	vWorker-&gt;createTables(vKeyFieldsUDO, vUdoEnt, vKeyFieldsVC, vVcEnt)
	#if (&lt;COUNTER&gt; = 0)
		vViewSQL = "create view %%(vViewName) AS %%^"
	#else
		vViewSQL = $concat(vViewSQL, "%%^%%^UNION%%^")
	#endif
	vViewSQL = $concat(vViewSQL, lpCreateViewFragment(vKeyFieldsUDO, vUdoEnt, vKeyFieldsVC, vVcEnt, vSpecs, "&lt;ITEM&gt;"))
	deleteInstance vWorker
#endfor

	SQL "drop view %%(vViewName)", "IDF"
	SQL vViewSQL, "IDF"

	commit

	putmess vViewSQL

end

entry lpCreateViewFragment
	returns string
	params
		string pKeysUDO : IN
		string pUdoEnt  : IN
		string pKeysVC  : IN
		string pVcEnt   : IN
		string pSpec    : IN
		string pUdoType : IN
	endParams
	variables
		string vSQL, vRelationshipJoin, vTimeStamp, vTimestampJoin, vTimestampField, vName, vNamespace
		numeric vCount
	endVariables

	vTimestampField = "UTIMESTAMP"
	while (pKeysUDO)
		if ( vRelationshipJoin )
			vRelationshipJoin = $concat( vRelationshipJoin, " AND ")
		endif

		if(!vName)
			vName = lpSQLID($itemnr(1,pKeysUDO), pSpec)
			vNamespace = "''"
		else
			vNamespace = lpSQLID($itemnr(1,pKeysUDO), pSpec)
		endif
			
			
		vRelationshipJoin = $concat(vRelationshipJoin, lpFormatRelationship($itemnr(1,pKeysUDO), pUdoEnt, $itemnr(1,pKeysVC), pVcEnt, "=", pSpec))
		delitem pKeysUDO, 1
		delitem pKeysVC, 1
	endWhile
	vTimestampJoin = lpFormatRelationship(vTimestampField, pUdoEnt, vTimestampField, pVcEnt, "=", pSpec)

	return "select %%(vName) AS %%(lpSQLID("NAME", pSpec)), %%^%\
    %%(vNamespace) AS %%(lpSQLID("NAMESPACE", pSpec)), %%^%\
    '%%(pUdoType)' AS %%(lpSQLID("TYPE", pSpec)), %%^%\
    'mod' AS %%(lpSQLID("ACTION", pSpec)) %%^%\
from %%(lpSQLID(pUdoEnt, pSpec)) where not exists ( %%^%\
	select 'e' from %%(lpSQLID(pVcEnt, pSpec)) %%^%\
		where (%%(vRelationshipJoin) AND %%(vTimestampJoin))) %%^%\
UNION select %%(vName) AS %%(lpSQLID("NAME", pSpec)), %%^%\
    %%(vNameSpace) AS %%(lpSQLID("NAMESPACE", pSpec)), %%^%\
    '%%(pUdoType)' AS %%(lpSQLID("TYPE", pSpec)), %%^%\
    'del' AS %%(lpSQLID("ACTION", pSpec)) %%^%\
from %%(lpSQLID(pVcEnt, pSpec)) where not exists ( %%^%\
	select 'e' from %%(lpSQLID(pUdoEnt, pSpec)) %%^%\
		where (%%(vRelationshipJoin)))"
end

entry lpSQLID
	returns string
	params
		string pID   : IN
		string pSpec : IN
	endParams

	selectCase $itemnr(1, pSpec) 
		case "upper"
			pID = $uppercase(pId)
		case "lower"
			pID = $lowercase(pId)
	endselectcase
		
	return $concat($itemnr(2,pSpec), pID, $itemnr(3,pSpec))
end
	
entry lpFormatRelationship
	returns string
	params
		string pUdoField : IN
		string pUdoEnt   : IN
		string pVcField  : IN
		string pVcEnt    : IN
		string pOperator : IN
		string pSpec     : IN
	endParams

	pUdoField = lpSQLID(pUdoField, pSpec)
	pUdoEnt = lpSQLID(pUdoEnt, pSpec)
	pVcField = lpSQLID(pVcField, pSpec)
	pVcEnt =  lpSQLID(pVcEnt, pSpec)
	
	return "(%%(pUdoEnt).%%(pUdoField) %%(pOperator) %%(pVcEnt).%%(pVcField))"
end	
